<Window x:Class="MeetupManager.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Meetup event information extractor" Height="150" Width="500" MaxHeight="200" MaxWidth="800" MinHeight="125" MinWidth="400" Closing="ApplicationClosing">
    <Grid>
        <Label Height="28" HorizontalAlignment="Left" Margin="12,9,0,0" Name="label1" VerticalAlignment="Top" Width="55">Event Id</Label>
        <TextBox Height="23" Margin="66,11,0,0" Name="txbEventId" VerticalAlignment="Top" HorizontalAlignment="Left" Width="82" />
        <TextBox Height="23" Margin="208,11,12,0" Name="txbAPIKey" VerticalAlignment="Top" />
        <Label Height="28" Margin="152,9,0,0" Name="lblApiKey" VerticalAlignment="Top" HorizontalAlignment="Left" Width="50">Api Key</Label>
        <Button Margin="12,49,12,9" Name="btnExport" Click="btnExport_Click">Load RSVP Data and Export</Button>
    </Grid>
</Window>
